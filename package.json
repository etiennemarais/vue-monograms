  "unpkg": "dist/vue-monograms.js",
  "module": "index.js",
  "author": {
    "name": "Etienne Marais",
    "email": "hello@etiennemarais.co.za"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etiennemarais/vue-monograms.git"
  },
  "bugs": {
